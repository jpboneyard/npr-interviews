	}//mq1150

	@media (min-width: 1400px) {
		max-width: $lg-width;
	}//mq1400
}

.img-full {
	width: 112%;
	margin-left: -6%;
	margin-bottom: 30px;
}

.full-m {
	width: 220%;
	margin-left: -60%;

	@media (min-width: 600px) {
		width: 110%;
		margin-left: -5%;
	}//mq
}

.img-first {
	margin-top: 30px;
}

.img-ftr-top {
	margin-bottom: -70px;

	@media (min-width: 450px) {
		margin-bottom: -100px;
	}//mq
}

.section-end {
	margin-bottom: 45px;

	@media (min-width: 700px) {
		margin-bottom: 60px;
	}
}

.section-start {
	margin-top: 45px;

	@media (min-width: 700px) {
		margin-top: 60px;
	}//mq
}

.site-width-img {
	width: 100%;
	max-width: none;
}

.site-width-mod {
	max-width: 200%;
}

.img-site-width {
