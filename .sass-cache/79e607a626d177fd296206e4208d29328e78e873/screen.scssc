/* ============================================= */
/*	Universal						   			 */ 
/* ============================================= */

a 		{ 	text-decoration: none; color: inherit; }
strong	{	font-weight: bold;	}
em   	{	font-style: italic;	}

a {

}

body {
	overflow: hidden;
}

h1,h2,h3,h4,h5 {
	-webkit-font-smoothing: antialiased;
}

h1 {
	margin: 0 auto;
	font: 400 70px/90% $sans-title;
	max-width: $standard-width;
	color: #000;
	text-transform: uppercase;
	letter-spacing: 3px;
	text-align: left;

	@media (min-width: 500px) {
		font-size: 100px;
	}//mq
}

h3 {
	margin: 0 auto;
	margin-bottom: 6px;
	color: #333;
	max-width: $standard-width;
	font: 700 20px/140% $sans-title;
	text-transform: uppercase;
	letter-spacing: 1px;

	@media (min-width: 500px) {
		font: 700 24px/140% $sans-title;
	}//mq

}

h4 {
	font: 400 22px/140% $sans-title;	
	text-transform: uppercase;
	letter-spacing: 2px;
}

.overline {
	margin: 0 auto;
	max-width: $standard-width;
	text-transform: uppercase;
	letter-spacing: 3px;
	font: 400 10px/170% $sans-title;
	color: $site-spot;
	margin-bottom: 8px;
	-webkit-font-smoothing: antialiased;

	@media (min-width: 500px) {
		font-size: 11px;
	}//mq
}

.intro-overline {
	margin: 0 auto;
	text-transform: uppercase;
	letter-spacing: 3px;
	font: 400 12px/170% $sans-title;
	color: $site-spot;
	margin-bottom: 8px;
	-webkit-font-smoothing: antialiased;
	text-align: center;

	@media (min-width: 600px) {
		font-size: 11px;
	}//mq
}

.intro-meta {
	margin: 0 auto;
	font: 400 18px/170% $sans-title;
	color: #444;
	margin-bottom: 6px;
	-webkit-font-smoothing: antialiased;
	text-align: left;
	max-width: $standard-width;
	letter-spacing: 2px;

	@media (min-width: 600px) {
		font-size: 18px;
	}//mq
}

.sponsor-line {
	font: 400 15px/120% $sans-title;
	color: #222;
	-webkit-font-smoothing: antialiased;

	@media (min-width: 800px) {
		font-size: 22px;
	}//mq800
}

.sponsor-line-b {
	font: 400 15px/120% $sans-title;
	color: #222;
	text-decoration: underline;

	@media (min-width: 800px) {
		font-size: 22px;
	}//mq800
}

.section-intro-h {
	font: 600 43px/113% $serif;
	color: #000;

	@media (min-width: 800px) {
		font-size: 60px;
		line-height: 170%;
	}//mq800
}//section-intro-h

p {
	-webkit-font-smoothing: antialiased;

	a {
		color: $site-spot;
		@include trans;

		&:hover {
			@include trans;
			color: $site-black;
		}//hover
	}//a
}//p

li {
	a {
		color: #000;
		@include trans;

		&:hover {
			@include trans;
			color: $site-black;
		}//hover
	}//a
}//li

.lead-in-txt {
