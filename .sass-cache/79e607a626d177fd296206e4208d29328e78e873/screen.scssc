/* ============================================= */
/*	Header							   			 */ 
/* ============================================= */

header {
	background-color: #fff;
	position: relative;
	z-index: 4;
	height: 65px;
	position: fixed;
	width: 100%;

	@media (min-width: 760px) {
		@include opacity (0.9);
	}//mq760

	@media (min-width: 900px) {
		padding: 22px 72px;

	}//mq900
}

.headroom {
	transition: transform 75ms linear;
}

.headroom--pinned {
    transform: translateY(0%);
    @include trans;
}

.headroom--unpinned {
     transform: translateY(-100%);
     @include trans;
}

.nav-chrome {
	height: 65px;
	width: 100%;
	padding: 22px 22px;
	position: fixed;
	top: 0px;
	left: 0px;
	z-index: 4;
	background-color: #fff;
	border-bottom: 1px solid;
	border-color: #E2E3E4;

	@media (min-width: 760px) {
		padding: 22px px;
		z-index: 2;
	}//mq760
}

.nav-logo-mod {
	float: left;
	padding-left: 0px;

	@media (min-width: 760px) {
		padding-top: 0px;
		padding-left: 52px;
	}//mq760

	img {
		max-width: 115px;
		width: 100%;

		@media (min-width: 760px) {
			max-width: 115px;
		}//mq760
	}//img
}

.nav-title {
	float: left;
	font: 200 1px/0%;
	max-width: 1px;
	text-indent: -20000px;	
}

.nav-link-mod {
	float: right;
	display: block;
	margin-top: -300px;

	@media (min-width: 760px) {
		padding-top: 3px;
		margin-top: 0px;
	}//mq760
}

.nav-link-item {
	display: inline;
	margin-right: 20px;
	font: 400 22px/140% $sans-title;
	@include trans;
	text-transform: uppercase;
	letter-spacing: 2px;
