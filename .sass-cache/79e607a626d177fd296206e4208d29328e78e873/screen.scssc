.end-tz-title {
	font: 800 22px/126% $serif;
	color: #eee;

	@media (min-width: 1200px) {
		font-size: 32px;
	}//mq800	

	@media (min-width: 1100px) and (max-width: 1799px){		

	}//mq	

	@media (min-width: 800px) {

	}//mq800

	@media (min-width: 1500px) and (max-width: 1800px){		
		font-size: 28px;
	}//mq	

	@media (min-width: 1801px) {

	}//mq1801

}

.overline-end-tz {
	margin-bottom: -2px;
	font-size: 14px;

	@media (min-width: 1200px) {
		font-size: 17px;
		margin-bottom: 0px;
	}//mq800	
}

.end-tz-txt {
	font: 400 15px/130% $serif;
	color: #eee;
	display: none;
	max-width: 90%;
	text-align: left;
	margin: 0 auto;
	margin-top: 12px;
	border-top: 1px solid;
	border-color: #fff;
	padding-top: 12px;
	@include trans;
	overflow: hidden;

	@media (min-width: 800px) {
		font-size: 20px;
		width: 80%;
	}//mq800

	@media (min-width: 2000px) {
		max-width: 470px;
	}//mq2000

	@media (min-width: 1500px) and (max-width: 1800px){		
		font-size: 18px;
	}//mq	
}

.end-tz-more {
	font: 400 12px/130% $serif;
	color: #eee;
	font-style: italic;
	border-top: 0px;
	text-align: center;
	color: $site-orange;

	@media (min-width: 800px) {
		font-size: 15px;
	}//mq800
}

.end-tz-txt-sub {
	font: 400 16px/126% $sans-title;
	color: #fff;
	text-transform: uppercase;
	letter-spacing: 3px;
}


