	@media (min-width: 800px) {
		font-size: 21px;
	}//mq800
}

.section-intro-h {
	font: 600 43px/113% $serif;
	color: #000;

	@media (min-width: 800px) {
		font-size: 60px;
		line-height: 170%;
	}//mq800
}//section-intro-h

p {
	-webkit-font-smoothing: antialiased;

	a {
		color: $site-spot;
		@include trans;

		&:hover {
			@include trans;
			color: $site-black;
		}//hover
	}//a
}//p

li {
	a {
		color: #000;
		@include trans;

		&:hover {
			@include trans;
			color: $site-black;
		}//hover
	}//a
}//li

.lead-in-txt {
	font: 800 24px/145% $serif;
