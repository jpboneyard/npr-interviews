/* ============================================= */
/*	Tab Navigation								 */ 
/* ============================================= */

.previous-tab {
	position: fixed;
	top: 50%;
	left: 20px;
	float: left;
	text-align: right;
	border: 1px solid;
	padding: 20px;
	padding-right: 16px;
	margin-left: -270px;
	min-width: 300px;
	@include trans;
	background-color: #fff;
	display: none;
	font-weight: bold;
	font: 400 15px/126% $sans;
	-webkit-font-smoothing: antialiased;
	z-index: 4;
	border-color: #E2E3E4;
	color: #666;

	@media (min-width: 1000px) {
		display: block;
	}//mq800

	&:hover {
		margin-left: -50px;
		@include trans;
		color: $site-orange;
	}

	a {
		padding: 20px;
		padding-left: 170px;
		margin: -20px;
		margin-left: -170px;
	}
}

.next-tab {
	position: fixed;
	top: 50%;
	right: 20px;
	float: right;
	border: 1px solid;
	margin-right: -270px;
	padding: 20px;
	padding-left: 16px;
	min-width: 300px;
	@include trans;
	background-color: #fff;
	display: none;
	font: 400 15px/126% $sans;
	-webkit-font-smoothing: antialiased;
	z-index: 4;
	border-color: #E2E3E4;
	color: #666;
	
	@media (min-width: 1000px) {
		display: block;
	}//mq800

	&:hover {
		margin-right: -50px;
		@include trans;
		color: $site-orange;
	}

	a {
		padding: 20px;
		padding-right: 170px;
		margin: -20px;
		margin-right: -170px;
	}
}

.tab-arrow {
	font: 400 18px/126% $sans;
	-webkit-font-smoothing: antialiased;	
	margin-right: 12px;
}

.left-arrow {
	margin-right: 0px;
	margin-left: 12px;
}

.tab-start {
	@include opacity(0);
}


.tab-fade{
	animation: fadein-3 1s;	
	-webkit-animation:fadein-3 1s;
}

@-webkit-keyframes fadein-3 {
  0%   { @include opacity(0); }
  75%   { @include opacity(0); } 
  100% { @include opacity(1); }
}

@-moz-keyframes fadein-3 {
  0%   { @include opacity(0); }
  75%   { @include opacity(0); } 
  100% { @include opacity(1); }
}


