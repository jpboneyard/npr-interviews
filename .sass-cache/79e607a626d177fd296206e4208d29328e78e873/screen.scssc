	z-index: -1;
	padding-top: 65px;
	background-color: #181818;

	@media (min-width: 400px) and (max-height: 1200px)  {
		padding-bottom: 65%;
	}

	@media (min-width: 800px) and (max-height: 1400px) {
		padding-bottom: 65%;
	}

	@media (min-width: 1200px) and (max-height: 1100px) {
		padding-bottom: 65%;
	}

	@media (min-width: 1400px) and (max-height: 1300px) {
		padding-bottom: 58%;
	}

	@media (min-width: 1400px) and (max-height: 1400px) {
		padding-bottom: 62.5%;	
	}

	@media (min-width: 1500px) and (max-height: 1400px) {
		padding-bottom: 52%;	
	}

	@media (min-width: 1900px) and (max-height: 1400px) {
		padding-bottom: 50%;	
	}

	@media (min-width: 2000px) and (max-height: 1400px) {
		padding-bottom: 55%;	
	}

	@media (min-width: 2200px) and (max-height: 1400px) {
		padding-bottom: 55%;	
	}	

	@media (min-width: 2300px) and (max-height: 1400px) {
