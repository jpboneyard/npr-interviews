	}

	@media (min-width: 1400px) and (max-height: 1300px) {
		padding-bottom: 58%;
	}

	@media (min-width: 1400px) and (max-height: 1400px) {
		padding-bottom: 62.5%;	
	}

	@media (min-width: 1500px) and (max-height: 1400px) {
		padding-bottom: 52%;	
	}

	@media (min-width: 1900px) and (max-height: 1400px) {
		padding-bottom: 50%;	
	}

