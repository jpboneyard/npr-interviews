/* ============================================= */
/*	Footer										 */ 
/* ============================================= */

footer {
	background-color: #181818;
	position: relative;
	color: #eee;
	padding: 60px 25px 70px 25px;
	z-index: 10;

	@media (min-width: 900px) {
		padding: 85px;
	}//mq
}

.ftr-content-mod {
	margin: 0 auto;
	max-width: 700px;
	@include pie-clearfix;

	@media (min-width: 900px) {
		max-width: 935px;
	}//mq
}

.ftr-link-list {
	padding-bottom: 11px;
	float: none;
	@include pie-clearfix;
	text-align: center;

	@media (min-width: 550px) {
		float: left;
		text-align: left;
	}//mq
}

.ftr-link-item {
	display: inline;
	font: 400 21px/126% $sans-title;
	-webkit-font-smoothing: antialiased;	
	text-transform: uppercase;
	letter-spacing: 2px;
	margin-right: 20px;
	padding-bottom: 12px;
	padding-top: 15px;

	&:last-child {
		border-right: 0px solid;
		border-color: #292929;
		margin-right: 20px;
