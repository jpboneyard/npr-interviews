/* ============================================= */
/*	Home										 */ 
/* ============================================= */

.home-intro-mod {
	position: relative;
}

.home-intro:before {
	content: '\0020';
	background: url(../_img/home-intro-bite-in.png) 50% 50% repeat;
	display: none;
	height: 12px;
	width: 100%;
	position: absolute;
	top: -12px;
	left: 0px;
	z-index: 4;	

	@media (min-width: 700px) {
		display: block;
	}//mq700

	@media (min-width: 1200px) {
		display: block;
		height: 24px;
		top: -22px;
	}//mq1200
}

.home-intro {
	background-color: #fff;
	margin: 0 auto;
	padding: 25px 15px; 
	margin-top: -25px;
	width: 90%;

	@media (min-width: 600px) {
		padding: 20px 15px;
	}

	@media (min-width: 700px) {
		margin-top: 0px;
		width: 100%;
	}

	h2 {
		margin: 0 auto;
		text-align: center;
		font: 400 23px/136% $serif;
		max-width: 860px;
		margin-bottom: 10px;

		a {
			color: #333332;

			&:hover { 
				color: $site-spot;
			}//hover
		}//a

		@media (min-width: 600px) {
			font-size: 41px;
			line-height: 125%;
		}//mq
	}//h2

	p {
		font: 400 20px/150% $sans-title;
