	width: 110%;

	@media (min-width: 800px) {
		position: fixed;
		top: 0px;
		right: 0;
		left: 0;
	}//mq800
}

.scroll-arrow-mod {
    display: none;
    margin: 0 auto;
    text-align: center;

	@media (min-width: 1000px) {
	    display: block;
	    margin: 0 auto;
	    position: absolute;
	    left: 0;
	    right: 0;
    	bottom: 5%;  
  	}//mq

}  

.scroll-down {
  color: #111;
  font-size: 19px;
  text-transform: uppercase;
  letter-spacing: 2px;
}

.scroll-arrow {
  max-width: 19px;
  margin-top: 8px;
}

.scroll-txt {
	font: 400 13px/126% $sans;
	text-transform: uppercase;
	letter-spacing: 2px;
	color: #fff;
}

.arrow-start {
	@include opacity(1);
	@include trans;
}

.arrow-fade {
	animation: fadeout-1 1s;	
	fill-mod: forwards;
	-webkit-animation:fadeout-1 1s;
	-webkit-animation-fill-mode: forwards;
}

@-webkit-keyframes fadeout-1 {
  0%   { @include opacity(1); }
  50%   { @include opacity(1); }
  100% { @include opacity(0); }
}

@-moz-keyframes fadeout-1 {
  0%   { @include opacity(1); }
  50%   { @include opacity(1); }
  100% { @include opacity(0); }
}

