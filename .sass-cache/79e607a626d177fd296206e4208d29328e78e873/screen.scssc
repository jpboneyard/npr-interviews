	margin-bottom: 35px;
	width: 103%;

	&:first-child {
		margin-left: -3%;
	}
}

.img-grid-2-mod {
	max-width: 1200px;
	margin: 0 auto;
}

.img-grid-2 {
	@include media-grid(2, 3%);
