	img {
		width: 200%;
	}
}

.project-details {
	max-width: $standard-width;
	width: 100%;
	display: block;
	margin: 0 auto;
}

.site-video-poster {
	display: block;

	@media (min-width: 1025px) {
		display: none;
		width: 200%;
