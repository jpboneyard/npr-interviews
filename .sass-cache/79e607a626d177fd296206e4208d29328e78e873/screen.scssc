/* ============================================= */
/*	Header							   			 */ 
/* ============================================= */

header {
	background-color: #fff;
	position: relative;
	z-index: 4;
	height: 65px;
	position: fixed;
	width: 100%;

	@media (min-width: 760px) {
		@include opacity (0.9);
	}//mq760

	@media (min-width: 900px) {
		padding: 22px 72px;

	}//mq900
}

.headroom {
	transition: transform 75ms linear;
}

.headroom--pinned {
    transform: translateY(0%);
    @include trans;
}

.headroom--unpinned {
     transform: translateY(-100%);
     @include trans;
}

.nav-chrome {
	height: 65px;
	width: 100%;
	padding: 22px 22px;
	position: fixed;
	top: 0px;
	left: 0px;
	z-index: 4;
	background-color: #fff;
	border-bottom: 1px solid;
	border-color: #E2E3E4;

	@media (min-width: 760px) {
		padding: 22px px;
		z-index: 2;
	}//mq760
}

