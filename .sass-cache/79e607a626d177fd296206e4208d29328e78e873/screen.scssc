.scroll-down {
  color: #111;
  font-size: 19px;
  text-transform: uppercase;
  letter-spacing: 2px;
}

.scroll-arrow {
  max-width: 19px;
  margin-top: 8px;
}

.scroll-txt {
	font: 400 13px/126% $sans;
	text-transform: uppercase;
	letter-spacing: 2px;
	color: #fff;
}

.arrow-start {
	@include opacity(1);
	@include trans;
}

.arrow-fade {
	animation: fadeout-1 1s;	
	fill-mod: forwards;
	-webkit-animation:fadeout-1 1s;
	-webkit-animation-fill-mode: forwards;
}

@-webkit-keyframes fadeout-1 {
  0%   { @include opacity(1); }
  50%   { @include opacity(1); }
  100% { @include opacity(0); }
}

@-moz-keyframes fadeout-1 {
  0%   { @include opacity(1); }
  50%   { @include opacity(1); }
  100% { @include opacity(0); }
}

