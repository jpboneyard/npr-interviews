.ftr-content-mod {
	margin: 0 auto;
	max-width: 935px;
}

.ftr-link-list {
	border-bottom: 1px solid;
	border-color: #fff;
	padding-bottom: 20px;
}

.ftr-link-item {
	display: inline;
	font: 400 24px/126% $sans-title;
	-webkit-font-smoothing: antialiased;	
	text-transform: uppercase;
	letter-spacing: 2px;
	margin-right: 20px;

	a {
		color: #fff;
	}
}

.ftr-info-mod {
	max-width: 735px;
	width: 100%;
	float: left;
	display: inline;
	@include pie-clearfix;
	border-right: 0px solid;
	padding: 15px 0px;

	@media (min-width: 1100px) {
		border-right: 1px solid;
	}//mq
}

.ftr-blurb-mod {
	padding: 20px 30px 0px 0px;
}

.ftr-blurb {
	font: 400 16px/130% $sans;
	-webkit-font-smoothing: antialiased;	
	display: inline;
}

.npr-ftr-logo {
	max-width: 50px;
	margin-right: 10px;
	float: left;
}

.ftr-sponsor-mod {
	float: none;
	display: block;

	@media (min-width: 1100px) {
		float: right;
		display: inline;
		text-align: center;
		@include pie-clearfix;
		padding: 30px 0px 30px 30px;
	}//mq
}	

.ftr-sponsor {
	max-width: 165px;
	margin-bottom: 10px;
}

.ftr-sponsor-txt {
	font: 400 14px/130% $sans;
	-webkit-font-smoothing: antialiased;	
}

.ftr-logo-mod {
	margin: 0 auto;
	text-align: center;
	position: relative;
	z-index: 3;
	display: block;

	&:hover {
		.ftr-arrow {
			@include opacity(1);
			@include trans;
		}	
	}

	img {
		max-width: 92px;
	}
}

.ftr-logo {
	position: absolute;
	top: -40px;
	left: 50%;
	margin-left: -46px;
}
 
.social-list {
	margin: 0px 0px 20px 0px;
	text-align: center;
	padding-top: 60px;
}

.social-item {
	display: inline;
	margin-right: 15px;
	@include opacity(.7);
	@include trans;

	&:last-child {
		margin-right: 0px;
	}//lc

	&:hover {
		@include trans;
		@include opacity(1);
	}//hover
}//social-item

.social-item-img {
	max-width: 25px;
	width: 100%;
}

.copyright {
	text-transform: uppercase;
	letter-spacing: 1px;
	font-size: 8px;
	text-align: center;	
	color: #222;
}

.article-end-tz-mod {
	max-width: $standard-width;
	margin: 0 auto;
	text-align: center;
	margin-bottom: 75px;
}

.article-end-tz {
	@include media-grid(2, 3%);
	width: 103%;

	&:first-child {
		margin-left: -3%;
	}
}

.end-tz-img {
	width: 100%;
}

.article-end-item {

	&:hover {
		@include opacity(.8);
		@include trans;
	}//hover

}//article-end-item

.end-tz-txt-mod {
	background-color: #000;
	padding: 15px;
	z-index: 4;
	@include trans;
	@include opacity(1);
}

.end-tz-txt {
	font: 400 20px/126% $serif;
	background-color: #000;
	color: #fff;
}

.end-tz-txt-sub {
	font: 400 16px/126% $sans-title;
	color: #fff;
	text-transform: uppercase;
	letter-spacing: 3px;
}

.more-interviews {
	max-width: $standard-width;
	margin: 0 auto;
	padding: 10px 5px;
	font: 400 26px/126% $sans-title;
	text-transform: uppercase;
	letter-spacing: 3px;
	text-align: left;
	border-top: 4px double;
}


/* ============================================= */
/*	404 Page				 					 */ 
/* ============================================= */


.error-video-mod {
	width: 120%;
	margin: -20%, 0%, 0%, -20%;
}

.error-video {
	position: fixed; right: 0; bottom: 0;
	min-width: 100%; min-height: 100%;
	width: auto; height: auto; z-index: -100;
	background: url(polina.jpg) no-repeat;
	background-size: cover;
}

.error-msg-mod {
	margin-top: 28%;
}

.error-msg {
	font-size: 50px;
	color: #eee;
	text-align: center;

	@media (min-width: 760px) {
		font-size: 100px;
	}
}

.error-tag {
	font-size: 20px;
	color: #ddd;
	text-align: center;
}

/* ============================================= */
/*	Button Style 								 */ 
/* ============================================= */

.btn-txt {
	text-transform: uppercase;
	letter-spacing: 2px;
}

.btn-submit{
	a {
		position: relative;
	    color: #fff;
	    text-decoration: none;
	    background-color: $site-green;
	    display: block;
	    padding: 14px 12px;
	    -webkit-border-radius: 8px;
	    -moz-border-radius: 8px;
	    border-radius: 8px;
	    -webkit-box-shadow: 0px 2px 0px rgba(149,178,94,.9), 0px 4px 10px rgba(0,0,0,.7);
	    -moz-box-shadow: 0px 2px 0px rgba(149,178,94,.9), 0px 4px 10px rgba(0,0,0,.7);
	    box-shadow: 0px 2px 0px rgba(149,178,94,.9), 0px 4px 10px rgba(0,0,0,.7);
	    margin: 40px auto;
		width: 180px;
		text-align: center;

		-webkit-transition: all .1s ease;
		-moz-transition: all .1s ease;
		-ms-transition: all .1s ease;
		-o-transition: all .1s ease;
		transition: all .1s ease;
	}

	a:hover {
		@include trans;
	    background-color: rgba(149,178,75,.9);
	}

	a:active {
	    -webkit-box-shadow: 0px 0px 0px rgba(30,150,144,.9), 0px 3px 6px rgba(0,0,0,.8);
	    -moz-box-shadow: 0px 0px 0px rgba(30,150,144,.9), 0px 3px 6px rgba(0,0,0,.8);
	    box-shadow: 0px 0px 0px rgba(30,150,144,.9), 0px 3px 6px rgba(0,0,0,.8);
	    position: relative;
	    top: 4px;
	}
}//btn-submit


