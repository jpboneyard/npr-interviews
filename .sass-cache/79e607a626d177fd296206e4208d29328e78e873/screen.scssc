	font: 400 15px/126% $sans;
	-webkit-font-smoothing: antialiased;
	z-index: 4;
	border-color: #E2E3E4;
	color: #666;

	@media (min-width: 800px) {
		display: block;
	}//mq800

	&:hover {
		margin-left: -50px;
		@include trans;
		color: #000;
	}

	a {
		padding: 20px;
		padding-left: 170px;
		margin: -20px;
