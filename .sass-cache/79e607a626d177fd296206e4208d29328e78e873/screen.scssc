.scroll-arrow-mod {
    display: none;
    margin: 0 auto;
    text-align: center;

	@media (min-width: 750px) {
	    display: block;
	    margin: 0 auto;
	    position: absolute;
	    left: 50%;
    	bottom: 5%;  
  	}//mq
}  

.scroll-down {
  color: #111;
  font-size: 19px;
  text-transform: uppercase;
  letter-spacing: 2px;
}

.scroll-arrow {
  max-width: 19px;
  margin-top: 8px;
}

.scroll-txt {
	font: 400 13px/126% $sans;
	text-transform: uppercase;
	letter-spacing: 2px;
	color: #fff;
}

.arrow-start {
	@include opacity(1);
}

.arrow-fade {
	animation: fadein-2 1s;	
	-webkit-animation:fadein-3 1s;
}

@-webkit-keyframes fadein-2 {
  0%   { @include opacity(1); }
  100% { @include opacity(0); }
}

@-moz-keyframes fadein-2 {
  0%   { @include opacity(1); }
  100% { @include opacity(0); }
}

