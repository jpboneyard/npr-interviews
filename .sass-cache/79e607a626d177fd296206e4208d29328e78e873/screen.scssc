/* ============================================= */
/*	Universal						   			 */ 
/* ============================================= */

a 		{ 	text-decoration: none; color: inherit; }
strong	{	font-weight: bold;	}
em   	{	font-style: italic;	}

a {

}

body {
	overflow: hidden;
}

h1,h2,h3,h4,h5 {
	-webkit-font-smoothing: antialiased;
}

h1 {
	margin: 0 auto;
	font: 400 70px/90% $sans-title;
	max-width: $standard-width;
	color: #000;
	text-transform: uppercase;
	letter-spacing: 3px;

	@media (min-width: 500px) {
		font-size: 82px;
	}//mq
}

h3 {
	margin: 0 auto;
	margin-bottom: 6px;
	color: #333;
	max-width: $standard-width;
	font: 700 20px/140% $sans-title;
	text-transform: uppercase;
	letter-spacing: 1px;

	@media (min-width: 500px) {
		font: 700 24px/140% $sans-title;
	}//mq

}

h4 {
	font: 400 22px/140% $sans-title;	
	text-transform: uppercase;
	letter-spacing: 2px;
}

.overline {
	margin: 0 auto;
	max-width: $standard-width;
	text-transform: uppercase;
	letter-spacing: 3px;
	font: 400 10px/170% $sans-title;
	color: $site-spot;
	margin-bottom: 8px;
	-webkit-font-smoothing: antialiased;

	@media (min-width: 500px) {
		font-size: 11px;
	}//mq
}

.intro-overline {
	margin: 0 auto;
	text-transform: uppercase;
	letter-spacing: 3px;
	font: 400 12px/170% $sans-title;
	color: $site-spot;
	margin-bottom: 8px;
	-webkit-font-smoothing: antialiased;
	text-align: center;

	@media (min-width: 600px) {
		font-size: 11px;
	}//mq
}

.intro-meta {
	margin: 0 auto;
	font: 400 18px/170% $sans-title;
	color: #444;
	margin-bottom: 6px;
	-webkit-font-smoothing: antialiased;
	text-align: center;
	max-width: $standard-width;
	letter-spacing: 2px;
	margin-top: 6px;

	@media (min-width: 600px) {
		font-size: 18px;
	}//mq
}

.sponsor-line {
	margin: 0 auto;
	font: 400 15px/120% $serif;
	color: #444;
	-webkit-font-smoothing: antialiased;
	text-align: center;
	display: inline;

	@media (min-width: 800px) {
		font-size: 21px;
	}//mq800
}

.sponsor-line-b {
	font: 600 15px/120% $serif;
/*	text-transform: uppercase;
	letter-spacing: 1px;*/
