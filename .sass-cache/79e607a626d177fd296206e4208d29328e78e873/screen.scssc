/* ============================================= */
/*	Interview 									 */ 
/* ============================================= */

.interview-mod-cover {
	background-size: cover;
	-webkit-background-size: cover;
	-moz-background-size: cover;
	-o-background-size: cover;
	background-attachment: fixed;
	min-height: 75%;
	width: 100%;
	z-index: 66;
	position: fixed;
	@include pie-clearfix;
}

.interview-mod {
	max-height: 675px;

	img {
		width: 100%;
	}
}

.interview-img {
	width: 100%;
}

.interview-mod-article {
	@include pie-clearfix;
	position: relative;
	height: 0;
	padding-bottom: 70.5%;
