	@media (min-width: 1150px) {
		max-width: $med-width;
	}//mq1150

	@media (min-width: 1400px) {
		max-width: $lg-width;
	}//mq1400
}

.img-full {
	width: 112%;
	margin-left: -6%;
	margin-bottom: 30px;
}

.full-m {
	width: 220%;
	margin-left: -60%;

	@media (min-width: 600px) {
		width: 110%;
		margin-left: -5%;
	}//mq
}

.img-first {
	margin-top: 30px;
}

.img-ftr-top {
	margin-bottom: -70px;

	@media (min-width: 450px) {
		margin-bottom: -100px;
	}//mq
}

.section-end {
	margin-bottom: 45px;

	@media (min-width: 700px) {
		margin-bottom: 60px;
	}
}

.section-start {
	margin-top: 45px;

	@media (min-width: 700px) {
		margin-top: 60px;
	}//mq
}

.site-width-img {
	width: 100%;
	max-width: none;
}

.site-width-mod {
	max-width: 200%;
}

.img-site-width {
	max-width: 200%;

	img {
		width: 200%;
	}
}

.project-details {
	max-width: $standard-width;
	width: 100%;
	display: block;
	margin: 0 auto;
}

.site-video-poster {
	display: block;

	@media (min-width: 1025px) {
		display: none;
		width: 200%;
	}//mq
}


/* ============================================= */
/*	Tab Navigation								 */ 
/* ============================================= */

.previous-tab {
	position: fixed;
	top: 50%;
	left: 20px;
	float: left;
	text-align: right;
	border: 1px solid;
	padding: 20px;
	padding-right: 16px;
	margin-left: -270px;
	min-width: 300px;
	@include trans;
	background-color: #fff;
	display: none;
	font-weight: bold;
	font: 400 15px/126% $sans;
	-webkit-font-smoothing: antialiased;
	z-index: 4;
	border-color: #E2E3E4;
	color: #666;

	@media (min-width: 1000px) {
		display: block;
	}//mq800

	&:hover {
		margin-left: -50px;
		@include trans;
		color: #000;
	}

	a {
		padding: 20px;
		padding-left: 170px;
		margin: -20px;
		margin-left: -170px;
	}
}

.next-tab {
	position: fixed;
	top: 50%;
	right: 20px;
	float: right;
	border: 1px solid;
	margin-right: -270px;
	padding: 20px;
	padding-left: 16px;
	min-width: 300px;
	@include trans;
	background-color: #fff;
	display: none;
	font: 400 15px/126% $sans;
	-webkit-font-smoothing: antialiased;
	z-index: 4;
	border-color: #E2E3E4;
	color: #666;
	
	@media (min-width: 1000px) {
		display: block;
	}//mq800

	&:hover {
		margin-right: -50px;
		@include trans;
		color: #000;
	}

	a {
		padding: 20px;
		padding-right: 170px;
		margin: -20px;
		margin-right: -170px;
	}
}

.tab-arrow {
	font: 400 18px/126% $sans;
	-webkit-font-smoothing: antialiased;	
	margin-right: 12px;
}

.left-arrow {
	margin-right: 0px;
	margin-left: 12px;
}

.tab-start {
	@include opacity(0);
}


.tab-fade{
	animation: fadein-3 1s;	
	-webkit-animation:fadein-3 1s;
}

@-webkit-keyframes fadein-3 {
  0%   { @include opacity(0); }
  75%   { @include opacity(0); } 
  100% { @include opacity(1); }
}

@-moz-keyframes fadein-3 {
  0%   { @include opacity(0); }
  75%   { @include opacity(0); } 
  100% { @include opacity(1); }
}


/* ============================================= */
/*	About										 */ 
/* ============================================= */

.about-signature-mod {
	max-width: $standard-width;
	margin: 0 auto;
}

.about-signature {
	max-width: 110px;
	margin: 0 auto;
	margin-bottom: 2px;
}



/* ============================================= */
/*	Footer										 */ 
/* ============================================= */

footer {
	background-color: #181818;
	position: relative;
	color: #eee;
	padding: 35px;
	height: 500px;
	z-index: 10;

	@media (min-width: 1100px) {
		height: 350px;
		padding: 85px;
	}//mq
}

.ftr-content-mod {
	margin: 0 auto;
	max-width: 935px;
}

.ftr-link-list {
	border-bottom: 2px solid;
	border-color: #292929;
	padding-bottom: 20px;
}

.ftr-link-item {
	display: inline;
	font: 400 21px/126% $sans-title;
	-webkit-font-smoothing: antialiased;	
	text-transform: uppercase;
	letter-spacing: 2px;
	margin-right: 20px;

	a {
		color: #777;
	}

	@media (min-width: 800px) {
		font-size: 24px;
	}//mq800
}

.ftr-info-mod {
	max-width: 735px;
	width: 100%;
	float: left;
	display: inline;
	@include pie-clearfix;
	border-right: 0px solid;
	border-color: #999;
	padding: 15px 0px;

	@media (min-width: 1100px) {
		border-right: 2px solid;
		border-color: #292929;
	}//mq
}

.ftr-blurb-mod {
	padding: 20px 30px 20px 0px;
	border-bottom: 1px solid;
	border-color: #999;
	color: #777;

	@media (min-width: 1000px) {
		border-bottom: 0px;
		padding-bottom: 0px;
	}//mq1000
}

.ftr-blurb {
	font: 400 14px/130% $sans;
	-webkit-font-smoothing: antialiased;	
	display: inline;

	@media (min-width: 800px) {
		font-size: 16px;
	}//mq800
}

.npr-ftr-logo {
	max-width: 55px;
	margin-right: 15px;
	margin-bottom: 30px;
	float: left;

	@media (min-width: 500px) {
		margin-bottom: 0px;
	}//mq600
}

.ftr-sponsor-mod {
	float: none;
	display: block;

	@media (min-width: 1100px) {
		float: right;
		display: inline;
		text-align: center;
		@include pie-clearfix;
		padding: 30px 0px 30px 30px;
	}//mq
}	

.ftr-sponsor {
	max-width: 165px;
	margin-bottom: 10px;
}

.ftr-sponsor-txt {
	font: 400 14px/130% $sans;
	-webkit-font-smoothing: antialiased;	
	color: #777;
}

.ftr-logo-mod {
	margin: 0 auto;
	text-align: center;
	position: relative;
	z-index: 3;
	display: block;

	&:hover {
		.ftr-arrow {
			@include opacity(1);
			@include trans;
		}	
	}

	img {
		max-width: 92px;
	}
}

.ftr-logo {
	position: absolute;
	top: -40px;
	left: 50%;
	margin-left: -46px;
}
 
.social-list {
	margin: 0px 0px 20px 0px;
	text-align: center;
	padding-top: 60px;
}

.social-item {
	display: inline;
	margin-right: 15px;
	@include opacity(.7);
	@include trans;

	&:last-child {
		margin-right: 0px;
	}//lc

	&:hover {
		@include trans;
		@include opacity(1);
	}//hover
}//social-item

.social-item-img {
	max-width: 23px;
	width: 100%;

	@media (min-width: 800px) {
		max-width: 23px;
		margin-top: -7px;
	}//mq800
}

.copyright {
	text-transform: uppercase;
	letter-spacing: 1px;
	font-size: 8px;
	text-align: center;	
	color: #222;
}

.article-end-tz-mod {
	margin: 0 auto;
	text-align: center;
}

.article-end-tz {
	@include media-grid(2, 0%);
	width: 100%;
	border-top: 1px solid;
	border-color: #181818;
	position: relative;
	background-color: #fff;
	z-index: 10;

	&:first-child {
		margin-left: 0%;
	}
}

.more-interviews {
	margin: 0 auto;
	padding: 10px;
	font: 400 16px/126% $sans-title;
	text-transform: uppercase;
	letter-spacing: 3px;
	border: 1px solid;
	text-align: center;
	max-width: 144px;
	background-color: #fff;
	position: absolute;
	margin-left: auto;
	margin-right: auto;
	left: 0;
	right: 0;
	z-index: 10;
	top: -12%;
	pointer-events: none;

	@media (min-width: 800px) {
		top: 40%;
		max-width: 218px;
		font-size: 28px;
	}//mq800	
}


.end-tz-img {
	width: 100%;
}

.article-end-item {

	&:hover {
		@include opacity(.8);
		@include trans;
	}//hover

}//article-end-item

.end-tz-txt-mod {
	background-color: #292929;
	padding: 10px;
	z-index: 4;
	@include trans;
	@include opacity(1);
	border-right: 1px solid;
	border-color: #181818;

	@media (min-width: 800px) {
		padding: 20px;
	}//mq800
}

.end-tz-txt {
	font: 400 17px/126% $serif;
	color: #eee;

	@media (min-width: 800px) {
		font-size: 25px;
	}//mq800
}

.end-tz-txt-sub {
	font: 400 16px/126% $sans-title;
	color: #fff;
	text-transform: uppercase;
	letter-spacing: 3px;
}


/* ============================================= */
/*	404 Page				 					 */ 
/* ============================================= */


.error-video-mod {
	width: 120%;
	margin: -20%, 0%, 0%, -20%;
}

.error-video {
	position: fixed; right: 0; bottom: 0;
	min-width: 100%; min-height: 100%;
	width: auto; height: auto; z-index: -100;
	background: url(polina.jpg) no-repeat;
	background-size: cover;
}

.error-msg-mod {
	margin-top: 28%;
}

.error-msg {
	font-size: 50px;
	color: #eee;
	text-align: center;

	@media (min-width: 760px) {
		font-size: 100px;
	}
}

.error-tag {
	font-size: 20px;
	color: #ddd;
	text-align: center;
}

/* ============================================= */
/*	Button Style 								 */ 
/* ============================================= */

.btn-txt {
	text-transform: uppercase;
	letter-spacing: 2px;
}

.btn-submit{
	a {
		position: relative;
	    color: #fff;
	    text-decoration: none;
	    background-color: $site-orange;
	    display: block;
	    padding: 14px 12px;
	    -webkit-border-radius: 8px;
	    -moz-border-radius: 8px;
	    border-radius: 8px;
	    -webkit-box-shadow: 0px 2px 0px rgba(149,178,94,.9), 0px 4px 10px rgba(0,0,0,.7);
	    -moz-box-shadow: 0px 2px 0px rgba(149,178,94,.9), 0px 4px 10px rgba(0,0,0,.7);
	    box-shadow: 0px 2px 0px rgba(149,178,94,.9), 0px 4px 10px rgba(0,0,0,.7);
	    margin: 40px auto;
		width: 180px;
		text-align: center;

		-webkit-transition: all .1s ease;
		-moz-transition: all .1s ease;
		-ms-transition: all .1s ease;
		-o-transition: all .1s ease;
		transition: all .1s ease;
	}

	a:hover {
		@include trans;
	    background-color: rgba(149,178,75,.9);
	}

	a:active {
	    -webkit-box-shadow: 0px 0px 0px rgba(30,150,144,.9), 0px 3px 6px rgba(0,0,0,.8);
	    -moz-box-shadow: 0px 0px 0px rgba(30,150,144,.9), 0px 3px 6px rgba(0,0,0,.8);
	    box-shadow: 0px 0px 0px rgba(30,150,144,.9), 0px 3px 6px rgba(0,0,0,.8);
	    position: relative;
	    top: 4px;
	}
}//btn-submit


/* ============================================= */
/*	CSS Animations								 */ 
/* ============================================= */


.bounce {
	position: relative;
	-webkit-animation: moveX 1s linear 0s infinite alternate, moveY 1s linear 0s infinite alternate;
	-moz-animation: moveX 1s linear 0s infinite alternate, moveY 1s linear 0s infinite alternate;
	-o-animation: moveX 1s linear 0s infinite alternate, moveY 1s linear 0s infinite alternate;
	animation: moveX 1s linear 0s infinite alternate, moveY 1s linear 0s infinite alternate;
}

@-webkit-keyframes moveY {
  from { top: 0; } to { top: 8px; }
}
@-moz-keyframes moveY {
  from { top: 0; } to { top: 8px; }
}
@-o-keyframes moveY {
  from { top: 0; } to { top: 8px; }
}
@keyframes moveY {
  from { top: 0; } to { top: 8px; }
}


