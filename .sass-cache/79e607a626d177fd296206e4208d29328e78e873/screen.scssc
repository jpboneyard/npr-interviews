/* ============================================= */
/*	Header							   			 */ 
/* ============================================= */

header {
	background-color: #fff;
	position: relative;
	z-index: 4;
	height: 65px;
	position: fixed;
	width: 100%;

	@media (min-width: 760px) {
		@include opacity (0.9);
	}

	@media (min-width: 900px) {
		padding: 22px 72px;

	}//mq
}

.headroom {
	transition: transform 75ms linear;
}

.headroom--pinned {
    transform: translateY(0%);
    @include trans;
}

.headroom--unpinned {
     transform: translateY(-100%);
     @include trans;
}

.nav-chrome {
	height: 65px;
	width: 100%;
	padding: 22px 22px;
	position: fixed;
	top: 0px;
	left: 0px;
	z-index: 4;
	background-color: #fff;
	border-bottom: 1px solid;
	border-color: #E2E3E4;

	@media (min-width: 760px) {
		padding: 22px px;
		z-index: 2;
	}
}

.nav-logo-mod {
	float: left;
	padding-left: 20px;

	@media (min-width: 760px) {
		padding-top: 0px;
	}

	img {
		max-width: 115px;
		width: 100%;

		@media (min-width: 760px) {
			max-width: 115px;
		}//mq
	}//img
}

.nav-title {
	float: left;
	font: 200 1px/0%;
	max-width: 1px;
	text-indent: -20000px;	
}

.nav-link-mod {
	float: right;
	display: block;
	margin-top: -300px;

	@media (min-width: 760px) {
		padding-top: 3px;
		margin-top: 0px;
	}
}

.nav-link-item {
	display: inline;
	margin-right: 20px;
	font: 400 22px/140% $sans-title;
	@include trans;
	text-transform: uppercase;
	letter-spacing: 2px;
	color: #000;

	@media (min-width: 760px) {
		padding: 0px;
	}

	a {
		&:hover { 
			color: $site-green;
			@include trans;
			background-color: #ddd;

			@media (min-width: 760px) {
				background-color: #fff;
			}//mq
		}//hover
	}//a

	&:last-child {
		margin-right: 0px;
	}
}


/***  Mobile-Nav  ***/ 

.m-nav-toggle {
	max-width: 30px;
	width: 100%;
	padding-top: 17px;
	float: right;
	display: block;
	background-image: url(/_img/_icons/m-nav-icon.png);
	background-size: 25px;
	margin-top: 4px;

	@media (min-width: 760px) {
		display: none;
	}//mq
}

.visible {
	display: block;
	background-color: #F2f2f2;
	width: 100%;
	position: relative;
	z-index: 1;
	margin-top: 65px;
	-webkit-transition: margin-top 0.3s ease-in-out;
	-moz-transition: margin-top 0.3s ease-in-out;
	-o-transition: margin-top 0.3s ease-in-out;
	transition: margin-top 0.3s ease-in-out;

	.nav-link-item {
		display: block;
		border-bottom: 1px solid #ddd;
		margin-right: 0px;

		&:last-child {
			border-bottom: 0px;
		}//lc

		a {
			padding-left: 10px;
			display: block;
			padding: 30px 0px 30px 10px;
		}//a
	}//nav-link-item
}


#access {
	float: none;
	background-color: #000;
	position: relative;
	z-index: 2;

	@media (min-width: 760px) and (max-width: 900px) {
			padding: 20px 20px 0px 0px;

	}

	@media (min-width: 760px) {
		float: right;
		background-color: transparent;		
	}//mq

	#menu-main-nav {
		> li {
			display: block;
			border-bottom: 1px solid;
			border-color: #222;

			@media (min-width: 760px) {
				display: inline;
				border-bottom: 0px solid;
			}//mq

			a {
				@include inline-block;
				background-color: #262626;
				color: #fff;
				font: 600 15px/120% ;
				text-transform: uppercase;
				letter-spacing: 1px;
				margin-right: 0px;
				display: block;
				padding: 22px 6px 22px 20px;
				@include trans;

				&:last-child {
					margin-right: 0px;
					border-bottom: 0px;
				}

				&:hover {
					@include trans;
					color: red;
					background-color: #333;
				}//hover

				@media (min-width: 760px) {
					display: inline;
					margin-right: 3px;
					background-color: #000;
					padding: 2px 8px;
					border-bottom: 0px;

					&:hover {
						background-color: #121212;
					}//hover
				}//mq
			}//a	
		}//li
	}//menu-main-nav
}//#access 


