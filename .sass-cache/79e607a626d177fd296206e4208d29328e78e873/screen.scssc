	@media (min-width: 1100px) {
		border-right: 1px solid;
		border-color: #292929;
	}//mq
}

.ftr-blurb-mod {
	padding: 20px 0px 30px 0px;
	border-bottom: 1px solid;
	border-top: 1px solid;
	border-color: #292929;
	color: #777;
	margin: 0 auto;
	margin-top: 25px;

	@media (min-width: 550px) {
		margin-top: 40px;
	}//mq550

	@media (min-width: 1100px) {
		border-bottom: 0px;
		padding-bottom: 0px;
		padding-right: 30px;
	}//mq1100
}

.ftr-blurb {
	font: 400 15px/130% $sans;
	-webkit-font-smoothing: antialiased;	
	display: inline;

	@media (min-width: 700px) {
		font-size: 16px;
	}//mq800
}

.npr-ftr-logo {
